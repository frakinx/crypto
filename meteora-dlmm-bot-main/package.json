{
  "name": "meteora-copybot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "start": "node --env-file=.env --enable-source-maps dist/index.js",
    "dev": "tsx src/index.ts",
    "server": "concurrently -n \"WEB,BOT\" -c \"cyan,magenta\" \"tsx src/server.ts\" \"tsx src/index.ts\"",
    "server:web": "tsx src/server.ts",
    "server:bot": "tsx src/index.ts",
    "test-position-open": "tsx src/test-position-open.ts",
    "test-position-open:list-pools": "tsx src/test-position-open.ts --list-pools",
    "test-swap-conversion": "tsx src/test-swap-conversion.ts",
    "test-swap-conversion:popular": "tsx src/test-swap-conversion-extended.ts",
    "lint": "echo 'add your linter'",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.28.0",
    "@meteora-ag/dlmm": "^1.8.0",
    "@noble/hashes": "^1.5.0",
    "@solana/web3.js": "^1.95.6",
    "@types/express": "^5.0.5",
    "bn.js": "^5.2.2",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "p-retry": "^6.2.0",
    "undici": "^6.19.8",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bn.js": "^5.2.0",
    "@types/node": "^20.11.17",
    "@types/yargs": "^17.0.32",
    "concurrently": "^9.2.1",
    "patch-package": "^8.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.5.4"
  }
}
